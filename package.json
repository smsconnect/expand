{
  "name": "Expand",
  "version": "1.0.0",
  "description": "Expand - plugin to shrink and expand a div",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "uglify-js": "^2.6.2"
  },
  "scripts": {
    "info": "echo 'Expand - plugin to shrink and expand a div'",
    "build": "echo 'building' && babel assets/main.js --presets babel-preset-es2015 -o assets/babel.js",
    "lint": "echo 'linting' && jshint assets/main.js",
    "test": "echo 'testing' && mocha test/",
    "minify:js": "echo 'minify:js' && uglifyjs assets/babel.js -o dist/expand.min.js",
    "pretest": "npm run lint -s",
    "posttest": "npm run minify:js"
  },
  "author": "Hayden Wagner",
  "license": "ISC"
}
